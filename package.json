{
  "name": "@nattui/react-components",
  "version": "0.0.26",
  "description": "A collection of reusable React components built with TypeScript and CSS Modules",
  "keywords": [
    "components",
    "react",
    "ui"
  ],
  "homepage": "https://github.com/nattui/react-components#readme",
  "bugs": {
    "url": "https://github.com/nattui/react-components/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nattui/react-components.git"
  },
  "author": "Natt Nguyen <hello@natt.sh> (https://natt.sh)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "biome check",
    "check:fix": "biome check --write",
    "check:type": "tsc",
    "dev": "storybook dev --no-open --port 6006",
    "prepublishOnly": "bun run build",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@storybook/react-vite": "^10.0.4",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.0.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.3.0"
}
