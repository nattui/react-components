{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "!.next/cache/**",
        ".next/**",
        "dist/**",
        "storybook-static/**",
      ],
    },
    "dev": {
      "cache": false,
      "persistent": true,
    },
  },
}
